<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.precious_lord.travelapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ktravelersicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard"
            android:parentActivityName=".Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ForgotPassword"
            android:parentActivityName=".Login" />
        <activity
            android:name=".BookTicket"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".BookingStatus"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".AboutUs"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".Reschedule"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".CancelBooking"
            android:parentActivityName=".Dashboard" />
        <activity
            android:name=".UserProfile"
            android:parentActivityName=".Dashboard" />
        <activity android:name=".PayPal"></activity>
    </application>

</manifest>